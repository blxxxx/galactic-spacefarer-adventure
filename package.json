{
  "name": "galactic-spacefarer-adventure",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@sap/cds": "^9",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/cds-typer": ">=0.1",
    "@cap-js/cds-types": "^0.15.0",
    "@cap-js/sqlite": "^2",
    "@sap/cds-dk": "^9",
    "@types/node": "^24.0.0",
    "cds-plugin-ui5": "^0.13.0",
    "tsx": "^4",
    "typescript": "^5"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-galactic-spacefarer-explorer": "cds watch --open my.space.galacticspacefarerexplorer/index.html?sap-ui-xx-viewCache=false --livereload false"
  },
  "private": true,
  "imports": {
    "#cds-models/*": "./@cds-models/*/index.js"
  },
  "cds": {
    "requires": {
      "db": {
        "kind": "sqlite",
        "credentials": {
          "url": "db.sqlite"
        }
      },
      "auth": {
        "kind": "mocked",
        "users": {
          "alice": {
            "attr": {
              "originPlanet": "EARTH"
            }
          },
          "bob": {
            "attr": {
              "originPlanet": "MARS"
            }
          }
        }
      }
    }
  },
  "workspaces": [
    "app/*"
  ],
  "sapux": [
    "app/galactic-spacefarer-explorer"
  ]
}
